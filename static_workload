select N_NAME, R_NAME, sum(L_EXTENDEDPRICE * (1 - L_DISCOUNT)) as revenue from CUSTOMER, ORDERS, LINEITEM, SUPPLIER, NATION, REGION where C_CUSTKEY = O_CUSTKEY       and L_ORDERKEY = O_ORDERKEY       and L_SUPPKEY = S_SUPPKEY       and C_NATIONKEY = S_NATIONKEY       and S_NATIONKEY = N_NATIONKEY       and N_REGIONKEY = R_REGIONKEY       and R_NAME = 'AFRICA' group by N_NAME, R_NAME
select sum(L_EXTENDEDPRICE * L_DISCOUNT) as revenue from LINEITEM
select N_NAME, P_NAME, sum(L_EXTENDEDPRICE * (1 - L_DISCOUNT)) as amount from PART, SUPPLIER, LINEITEM, PARTSUPP, ORDERS, NATION where S_SUPPKEY = L_SUPPKEY       and PS_SUPPKEY = L_SUPPKEY       and PS_PARTKEY = L_PARTKEY       and P_PARTKEY = L_PARTKEY       and O_ORDERKEY = L_ORDERKEY       and S_NATIONKEY = N_NATIONKEY       and P_NAME like '%dark%' group by N_NAME, P_NAME
select C_CUSTKEY, C_NAME,        sum(L_EXTENDEDPRICE * (1 - L_DISCOUNT)) as revenue,        C_ACCTBAL, N_NAME, C_ADDRESS, C_PHONE, C_COMMENT, L_RETURNFLAG from CUSTOMER, ORDERS, LINEITEM, NATION where C_CUSTKEY = O_CUSTKEY       and L_ORDERKEY = O_ORDERKEY       and C_NATIONKEY = N_NATIONKEY       and L_RETURNFLAG = 'A' group by C_CUSTKEY, C_NAME, C_ACCTBAL, C_PHONE, N_NAME, C_ADDRESS, C_COMMENT, L_RETURNFLAG
select OUT_COUNT, count(*) as custdist from (select C_CUSTKEY, count(*) as C_COUNT       from CUSTOMER left outer join ORDERS       on C_CUSTKEY = O_CUSTKEY       group by C_CUSTKEY) as c_orders (C_CUSTKEY, OUT_COUNT) group by OUT_COUNT
select P_BRAND, P_TYPE, P_SIZE,        count(*) as supplier_cnt from PARTSUPP, PART where P_PARTKEY = PS_PARTKEY       and P_BRAND <> 'Brand#12'       and P_SIZE = '48'       and P_TYPE not like 'LARGE%' group by P_BRAND, P_TYPE, P_SIZE
select P_BRAND, P_CONTAINER, sum(L_EXTENDEDPRICE / 7.0) as avg_year from LINEITEM, PART where P_PARTKEY = L_PARTKEY       and P_BRAND = 'Brand#52'       and P_CONTAINER = 'JUMBO CASE' group by P_BRAND, P_CONTAINER
select C_NAME, C_CUSTKEY, O_ORDERKEY, O_ORDERDATE, O_TOTALPRICE, sum(L_QUANTITY) as quantity from CUSTOMER, ORDERS, LINEITEM where O_ORDERKEY = '3'       and C_CUSTKEY = O_CUSTKEY       and O_ORDERKEY = L_ORDERKEY group by C_NAME, C_CUSTKEY, O_ORDERKEY, O_ORDERDATE, O_TOTALPRICE
select L_RETURNFLAG, L_LINESTATUS,        sum(L_QUANTITY) as qty,        sum(L_EXTENDEDPRICE) as sum_base_price,        count(*) as count_order from lineitem l where L_LINESTATUS <> 'O' group by L_RETURNFLAG, L_LINESTATUS
select S_ACCTBAL, S_NAME, N_NAME, P_PARTKEY, P_MFGR, S_ADDRESS, S_PHONE, S_COMMENT, R_NAME, PS_SUPPLYCOST, P_SIZE, P_TYPE from PART, SUPPLIER, PARTSUPP, NATION, REGION where P_PARTKEY = PS_PARTKEY       and S_SUPPKEY = PS_SUPPKEY       and S_NATIONKEY = N_NATIONKEY       and N_REGIONKEY = R_REGIONKEY       and R_NAME = 'AFRICA'       and PS_SUPPLYCOST < '909.7'       and P_SIZE = '7'       and P_TYPE like '%SMALL%'
